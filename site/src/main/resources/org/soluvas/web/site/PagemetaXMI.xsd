        <xsd:annotation>
          <xsd:documentation>What to apply when the selector matches.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="positioner" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Guide:

* -100 : org.soluvas core
* -50 : module-level stuff (e.g. bippo salesorder)
* -10 : product-level stuff (e.g. bippo )
* 0 or missing : application-level stuff (e.g. berbatik)
* 10 : customizations
</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
