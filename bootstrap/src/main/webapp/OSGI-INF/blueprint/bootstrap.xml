<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.0.0"
    xmlns:ext="http://aries.apache.org/blueprint/xmlns/blueprint-ext/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wicket="http://www.ops4j.org/schema/wicket"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0
        http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.0.0 http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.0.0
        http://aries.apache.org/blueprint/xmlns/blueprint-ext/v1.0.0 http://aries.apache.org/blueprint/xmlns/blueprint-ext/v1.0.0
        http://karaf.apache.org/xmlns/shell/v1.0.0 http://karaf.apache.org/xmlns/shell/v1.0.0">

	<reference id="site" interface="org.soluvas.web.site.Site"/>
	
	<reference-list id="sidebarBlocks" interface="org.soluvas.web.site.ComponentFactory" availability="optional"
		filter="(region=sidebar)"/>
	<reference id="navbarBlock" interface="org.soluvas.web.site.ComponentFactory"
		filter="(region=navbar)"/>

	<reference-list id="cssLinks" interface="org.soluvas.web.site.CssLink" availability="optional"/>
	<reference-list id="headJavaScripts" interface="org.soluvas.web.site.JavaScriptLink" availability="optional"
		filter="(region=head)"/>
		
	<reference-list id="beforeFooterJsBlocks" interface="org.soluvas.web.site.ComponentFactory"
		filter="(region=before-footer-js)"/>
	<reference-list id="footerJavaScripts" interface="org.soluvas.web.site.JavaScriptLink" availability="optional"
		filter="(region=footer)"/>

</blueprint>
